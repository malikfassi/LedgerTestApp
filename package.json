{
  "name": "LedgerAppTest",
  "version": "0.1.0",
  "private": true,
  "main": "elect.js",
  "homepage": "./",
  "dependencies": {
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-router": "^4.1.2",
    "react-router-dom": "^4.1.2",
    "react-scripts": "1.0.10",
    "foreman": "^2.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "start-web": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "electron": "electron .",
    "start-electron": "nf start",
    "package-mac": "electron-packager . 'Ledger Test App' --platform=darwin --prune=true --out=release-builds --overwrite",
    "package-win": "electron-packager . --overwrite --asar=true --platform=win32 --arch=ia32 --prune=true --out=release-builds --version-string.ProductName=\"Legder Test App\"",
    "package-lin": "electron-packager . 'Ledger Test App' --overwrite --platform=linux --arch=x64 --prune=true --out=release-builds"
  },
  "devDependencies": {
    "electron": "^1.7.5",
    "eslint-plugin-flowtype": "^2.35.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.2.0",
    "gulp": "^3.9.1",
    "gulp-sass": "^3.1.0"
  }
}
